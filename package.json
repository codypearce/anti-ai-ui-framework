{
  "name": "anti-ai-ui",
  "version": "0.1.1",
  "description": "A framework for creating UI components that are resilient against AI browsers and automated interactions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vanilla": {
      "types": "./dist/vanilla/index.d.ts",
      "import": "./dist/vanilla/index.mjs",
      "require": "./dist/vanilla/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:analyze": "tsup --metafile && node -e \"console.log('Metafile written; inspect dist/*.meta.json or check sizes')\"",
    "demo": "npm run build && node scripts/serve.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest --run --pool=forks",
    "test:coverage": "vitest --run --pool=forks --coverage",
    "test:watch": "vitest --pool=forks",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ui",
    "react",
    "components",
    "anti-ai",
    "automation-resistant",
    "bot-detection",
    "user-verification",
    "engagement",
    "typescript",
    "vanilla-js",
    "human-interaction",
    "click-verification"
  ],
  "author": "anti-ai-ui contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codypearce/anti-ai-ui-framework.git"
  },
  "bugs": {
    "url": "https://github.com/codypearce/anti-ai-ui-framework/issues"
  },
  "homepage": "https://github.com/codypearce/anti-ai-ui-framework#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.57.1",
    "jsdom": "^24.1.3",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
